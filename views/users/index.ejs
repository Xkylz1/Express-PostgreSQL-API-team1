<table class="table table-striped">
  <th>name</th>
  <th>photo profile</th>
  <th>age</th>
  <th>phone number</th>
  <% users.map((user) => { %>
  <tr>
    <td><%= user.firstName %></td>
    <td><%= user.age %></td>
    <td><%= user.phoneNumber %></td>
    <td><img src="<%= user.photoProfile %>" class="rounded mx-auto d-block img-fluid"/></td>
  </tr>
  <% }) %>


  
</table>
